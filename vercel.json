{
  "version": 2,
  "routes": [
    {
      "src": "/api",
      "dest": "/api/message.js"
    },
    {
      "src": "/api",
      "dest": "/api/message-attach.js"
    }
  ],
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "nuxt.config.ts",
      "use": "@nuxtjs/vercel-builder",
      "config": {
        "serverFiles": [
          "api/**"
        ]
      }
    }
  ]
}
